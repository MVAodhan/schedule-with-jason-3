generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Episode {
  id                  Int     @id @default(autoincrement())
  sanityId            String
  date                String
  description         String
  guest               Json
  host                Json
  tags                Json
  slug                String
  title               String
  uri                 String
  timezone            String?
  chapters            String?
  links               Json?
  tech                String?
  demo                String?
  repo                String?
  twitter_description String?
}

model Schedule {
  id                  Int    @id @default(autoincrement())
  date                String
  description         String
  guest_name          String
  guest_twitter       String
  twitter_description String
  title               String
}
